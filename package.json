{
  "name": "ssh",
  "displayName": "IPLC C语言程序编译及下载",
  "description": "扩展插件，支持C语言程序的编译及下载",
  "publisher": "lb",
  "icon": "media/worker.png",
  "keywords": [
    "ssh",
    "服务器上传"
  ],
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onView:nodeDependencies",
    "onView:makeViews"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyLibraryCommand",
        "title": "动态库命令",
        "category": "ssh",
        "icon": {
          "light": "resources/copy_light.svg",
          "dark": "resources/copy_dark.svg"
        }
      },
      {
        "command": "setRemoteIp",
        "title": "设置远程IP",
        "category": "ssh",
        "icon": {
          "light": "resources/server_light.svg",
          "dark": "resources/server_dark.svg"
        }
      },
      {
        "command": "nodeDependencies.refreshEntry",
        "title": "刷新",
        "category": "ssh",
        "icon": {
          "light": "resources/refresh_light.svg",
          "dark": "resources/refresh_dark.svg"
        }
      },
      {
        "command": "nodeDependencies.uploadEntry",
        "title": "下载",
        "category": "ssh",
        "icon": {
          "light": "resources/upload_light.gif",
          "dark": "resources/upload_light.gif"
        }
      },
      {
        "command": "makeViews.selfCommand",
        "title": "本地编译",
        "category": "ssh",
        "icon": {
          "light": "resources/exec_light.png",
          "dark": "resources/exec_dark.png"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "copyLibraryCommand",
          "when": "view == nodeDependencies",
          "group": "navigation@2"
        },
        {
          "command": "setRemoteIp",
          "when": "view == nodeDependencies",
          "group": "navigation@1"
        },
        {
          "command": "nodeDependencies.refreshEntry",
          "when": "view == nodeDependencies",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "makeViews.selfCommand",
          "when": "view == makeViews",
          "group": "inline"
        },
        {
          "command": "nodeDependencies.uploadEntry",
          "when": "view == nodeDependencies",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "程序编译及下载",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "package-explorer": [
        {
          "id": "makeViews",
          "name": "本地编译",
          "icon": "media/dep.svg",
          "contextualTitle": "本地编译",
          "visibility":"visible",
          "type": "tree"
        },
        {
          "id": "nodeDependencies",
          "name": "程序下载",
          "icon": "media/dep.svg",
          "contextualTitle": "程序下载",
          "visibility":"visible",
          "type": "tree"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "archiver": "^5.0.2",
    "chalk": "^4.1.2",
    "inquirer": "^7.3.3",
    "node-ssh": "^12.0.4",
    "ora": "^5.1.0",
    "sudo-prompt": "^9.2.1"
  },
  "repository": {}
}
